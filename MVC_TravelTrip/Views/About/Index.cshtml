<!DOCTYPE HTML>
<html>

<body>


    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>About</h3>
                <img src="~/web/images/about_banner.jpg" alt="" />
               @foreach (var x in Model)
               {
                   //Model içerisinde hakkimizda listesi var
                   <p>
                       @x.Aciklama
                   </p>  

               }
            </div>
        </div>
    </div>

</body>
</html>


