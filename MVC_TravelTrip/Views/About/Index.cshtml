@using MVC_TravelTrip.Models.Siniflar
@model List<Hakkimizda>
    @*  Hakkimizda sınıfını listelemek için*@
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

